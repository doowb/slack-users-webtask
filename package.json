{
  "name": "slack-users-webtask",
  "description": "Webtask.io service for a slack badge showing the current and total users of a slack team.",
  "version": "0.1.0",
  "homepage": "https://github.com/doowb/slack-users-webtask",
  "author": "Brian Woodward (https://github.com/doowb)",
  "repository": "doowb/slack-users-webtask",
  "bugs": {
    "url": "https://github.com/doowb/slack-users-webtask/issues"
  },
  "license": "MIT",
  "files": [
    "dist",
    "index.js",
    "lib",
    "LICENSE",
    "templates"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "handlebars": "^4.0.5",
    "slack-users": "^0.1.0"
  },
  "devDependencies": {
    "brfs": "^1.4.3",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-bundle-webtask": "^0.1.0",
    "gulp-format-md": "^0.1.10",
    "mocha": "^3.0.2"
  },
  "keywords": [
    "slack",
    "users",
    "wt"
  ],
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "related": {
      "list": []
    },
    "reflinks": [
      "verb",
      "verb-generate-readme"
    ]
  }
}
