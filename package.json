{
  "name": "slack-users-wt",
  "description": "Webtask.io service for getting the users for a specified slack team.",
  "version": "0.1.0",
  "homepage": "https://github.com/doowb/slack-users-wt",
  "author": "Brian Woodward (https://github.com/doowb)",
  "repository": "doowb/slack-users-wt",
  "bugs": {
    "url": "https://github.com/doowb/slack-users-wt/issues"
  },
  "license": "MIT",
  "files": [
    "dist",
    "index.js",
    "lib",
    "LICENSE",
    "templates"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "handlebars": "^4.0.5"
  },
  "devDependencies": {
    "brfs": "^1.4.3",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-bundle-webtask": "^0.1.0",
    "gulp-format-md": "^0.1.10"
  },
  "keywords": [
    "slack",
    "users",
    "wt"
  ],
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "lint": {
      "reflinks": true
    },
    "related": {
      "list": []
    },
    "reflinks": [
      "verb",
      "verb-generate-readme"
    ]
  }
}